#include "matrix.h"
#include <stdio.h>
int main(){
  Matrix m1,m2,result;
  m1=mat_rand(3,3);
  m2=mat_O(4,4);
  mat_init(&result,2,2);
  mat_print(m1);
  putchar('\n');
  mat_print(m2);
  putchar('\n');
  mat_cp_lb(m2,m1);
  mat_print(m2);
  putchar('\n');
  mat_cp_lb(result,m1);
  mat_print(result);
  mat_free(&m1);
  mat_free(&m2);
  mat_free(&result);
  return 0;
}
